{
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F8F8F2"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#E6C000"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#C2FFDF"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#C2FFDF"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#C2FFDF"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#C2FFDF"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#FFF352"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#C2FFDF"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#C5A3FF"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#C2FFDF"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#C2FFDF"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#FFF352"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E6C000"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#C2FFDF"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#C5A3FF"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#FFF352"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#C2FFDF"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#C2FFDF"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "foreground": "#C2FFDF"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#C2FFDF"
      }
    }
  ],
  "colors": {
    "activityBar.background": "#716799",
    "editor.background": "#5A5475",
    "editor.foreground": "#F8F8F2",
    "editor.lineHighlightBackground": "#716799",
    "editor.selectionBackground": "#716799",
    "editorCursor.foreground": "#F8F8F0",
    "editorLineNumber.foreground": "#F9267275",
    "editorLineNumber.activeForeground": "#F92672",
    "editorWhitespace.foreground": "#F9267250",
    "menu.background": "#0076D6",
    "sideBar.background": "#716799",
    "sideBar.foreground": "#F8F8F2",
    "sideBarSectionHeader.background": "#716799",
    "tab.inactiveBackground": "#716799",
    "titleBar.activeBackground": "#0076D6",
    "titleBar.inactiveBackground": "#716799"
  },
  "name": "periwinkle"
}
